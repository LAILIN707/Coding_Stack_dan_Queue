public class NilaiMatkul {
    private String nim;
    private String nama;
    private double nilai;
    
    NilaiMatkul(){
        
    }
    void setNim(String nim){
        this.nim = nim;
        
    }
    String getNim(){
        return nim; 
    }
    
    void SetNama(String nama){
        this.nama = nama;
        
    }
    String getNama(){
        return nama;
    }
    
    void setNilai(double nilai){
        this.nilai = nilai;
        
    }
    double getNilai(){
        return nilai;
        
    }
    
}
